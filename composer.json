{
    "name": "freddokresna/yii2-admin-fkresna",
    "description": "RBAC Auth manager for Yii2 forked",
    "keywords": ["yii", "admin", "auth", "rbac"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/mdmsoft/yii2-admin/issues",
        "source": "https://github.com/mdmsoft/yii2-admin"
    },
    "authors": [
        {
            "name": "freddokresna",
            "email": "freddokresna@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "composer",
            "url": "https://repo.packagist.org"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.53"
    },
    "require-dev": {
         
    },
    "suggest": {
        "yiisoft/yii2-bootstrap": "Used when using layout 'left-menu', 'right-menu' or 'top-menu'"
    },
    "autoload": {
        "psr-4": {
            "mdm\\admin\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-1.0.master": "1.0.x-dev",
            "dev-master": "2.x-dev"
        },
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm"
        }
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
